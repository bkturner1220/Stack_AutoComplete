{
  "name": "stack_autocomplete",
  "version": "1.0.0",
  "description": "A Automotive stack overflow utility",
  "main": "index.js",
  "scripts": {
    "predeploy": "npm run build",
    "deploy": "gh-pages -d build",
    "build": "react-scripts build",
    "start": "concurrently \"cd server && npm start\" \"cd client && npm start\"",
    "install": "cd server && npm i --legacy-peer-deps && cd ../client && npm i --legacy-peer-deps"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/bkturner1220/Stack_AutoComplete.git"
  },
  "author": "",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/bkturner1220/Stack_AutoComplete/issues"
  },
  "homepage": "https://github.com/bkturner1220/Stack_AutoComplete#readme",
  "devDependencies": {
    "concurrently": "^7.0.0"
  },
  "dependencies": {
    "gh-pages": "^3.2.3",
    "react-scripts": "^5.0.0"
  }
}
